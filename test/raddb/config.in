# A minimum config file. See config.syntax for explanation of 
# the syntax.
#
option {
	usr2delay 30;
	max-requests 1024;
};

logging {
	channel debug {
		file "radius.debug";
	};
	channel auth {
		file "radius.log";
		print-cons yes;
	};
	category auth {
		channel "auth";
		print-auth yes;
		print-failed-pass yes;
	};
	category debug {
		channel debug;
	};
};

auth {
	port @AUTH_PORT@;
	max-requests 127;
	request-cleanup-delay 2;
	detail yes;
	strip-names yes;
	checkrad-assume-logged yes;
};

acct {
	port @ACCT_PORT@;
	max-requests 127;
	request-cleanup-delay 2;
};

proxy {
	max-requests 127;
	request-cleanup-delay 2;
};

